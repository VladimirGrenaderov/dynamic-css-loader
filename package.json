{
  "name": "dynamic-css-loader",
  "version": "1.0.0",
  "description": "Dynamic CSS loader useful for code splitting",
  "main": "dist/cjs.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "webpack-defaults": "webpack-defaults",
    "start": "yarn run serve:dev src",
    "build": "cross-env NODE_ENV=production babel -s true src -d dist --ignore 'src/**/*.test.js'",
    "clean:dist": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "yarn run clean:dist",
    "prepublish": "yarn run build",
    "release": "yarn run standard-version",
    "security": "nsp check",
    "serve:dev": "nodemon $2 --exec babel-node",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:coverage": "yarn run test:coverage",
    "travis:lint": "yarn run lint && yarn run security",
    "travis:test": "yarn run test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "cross-env": "^3.2.3",
    "del-cli": "^0.2.1",
    "eslint": "^3.17.1",
    "eslint-config-webpack": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.1",
    "loader-runner": "^2.3.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.0.0",
    "webpack-defaults": "^0.2.1"
  },
  "engines": {
    "node": ">=4.3 <5.0.0 || >=5.10"
  },
  "files": [
    "dist"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "loader-utils": "^1.0.3"
  }
}
